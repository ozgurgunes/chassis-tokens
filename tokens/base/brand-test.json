{
  "figma": {
    "switch": {
      "brand": {
        "mode-2": {
          "$type": "boolean",
          "$value": "true"
        }
      }
    }
  },
  "color": {
    "base": {
      "palette": {
        "neutral": {
          "main": {
            "$type": "color",
            "$value": "#757575"
          }
        },
        "danger": {
          "main": {
            "$type": "color",
            "$value": "#E02D12"
          }
        },
        "success": {
          "main": {
            "$type": "color",
            "$value": "#0B8A0B"
          }
        },
        "warning": {
          "main": {
            "$type": "color",
            "$value": "#996E09"
          }
        },
        "info": {
          "main": {
            "$type": "color",
            "$value": "#098294"
          }
        },
        "brand": {
          "main": {
            "$type": "color",
            "$value": "#1273E0"
          }
        },
        "accent": {
          "main": {
            "$type": "color",
            "$value": "#D115C5"
          }
        }
      }
    }
  },
  "typography": {
    "fontFamily": {
      "text": {
        "$type": "fontFamilies",
        "$value": "Helvetica, Arial, sans-serif"
      },
      "display": {
        "$type": "fontFamilies",
        "$value": "'Times New Roman', Times, serif"
      },
      "code": {
        "$type": "fontFamilies",
        "$value": "'Courier New', Courier, monospace"
      }
    },
    "fontWeight": {
      "text": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Light"
        }
      },
      "display": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Regular"
        }
      },
      "code": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Bold"
        }
      },
      "html": {
        "blockquote": {
          "$type": "fontWeights",
          "$value": "Light Oblique"
        }
      }
    }
  },
  "borderRadius": {
    "base": {
      "xsmall": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "small": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "medium": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "large": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "2xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "3xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "rare": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      }
    }
  }
}

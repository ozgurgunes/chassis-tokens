        "segment": {
          "default-item-active-bg": {
            "$type": "color",
            "$value": "{color.context.default.bg-button}"
          },
          "default-item-active-fg": {
            "$type": "color",
            "$value": "{color.context.default.fg-button}"
          },
          "alternate-item-active-bg": {
            "$type": "color",
            "$value": "{color.context.alternate.bg-button}"
          },
          "alternate-item-active-fg": {
            "$type": "color",
            "$value": "{color.context.alternate.fg-button}"
          }
        }
      }
    }
  },
  "space": {
    "common": {
      "modal": {
        "large-padding-y": {
          "$type": "spacing",
          "$value": "{space.context.xlarge}"
        },
        "large-padding-x": {
          "$type": "spacing",
          "$value": "{space.context.xlarge}"
        },
        "large-gap": {
          "$type": "spacing",
          "$value": "{space.context.3xlarge}"
        }
      },
      "page": {
        "large-padding-y": {
          "$type": "spacing",
          "$value": "{space.context.xlarge}"
        },
        "large-padding-x": {
          "$type": "spacing",
          "$value": "{space.context.5xlarge}"
        },
        "large-gap": {
          "$type": "spacing",
          "$value": "{space.context.3xlarge}"
        }
      }
    },
    "mobile": {
      "nav-top": {
        "segment": {
          "item-padding-v": {
            "$type": "spacing",
            "$value": "{space.context.xsmall}"
          },
          "item-padding-h": {
            "$type": "spacing",
            "$value": "{space.context.small}"
          },
          "frame-padding": {
            "$type": "spacing",
            "$value": "{space.base.0}"
          }
        }
      }
    }

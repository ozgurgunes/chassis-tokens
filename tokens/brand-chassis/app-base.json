        "path-outbound": {
          "$type": "color",
          "$value": "{color.common.map.path-ride}"
        },
        "point-active": {
          "$type": "color",
          "$value": "{color.context.default.indicator-main}"
        },
        "point-inactive": {
          "$type": "color",
          "$value": "{color.context.neutral.indicator-slight}"
        },
        "point-home": {
          "$type": "color",
          "$value": "{color.context.default.indicator-main}"
        },
        "point-male": {
          "$type": "color",
          "$value": "#08A8EE"
        },
        "point-female": {
          "$type": "color",
          "$value": "#E149A5"
        },
        "point-selected": {
          "$type": "color",
          "$value": "{color.context.neutral.indicator-main}"
        },
        "point-boarding": {
          "$type": "color",
          "$value": "{color.context.success.base-color}"
        },
        "point-landing": {
          "$type": "color",
          "$value": "{color.context.danger.base-color}"
        },
        "trip-start": {
          "$type": "color",
          "$value": "{color.context.success.base-color}"
        },
        "trip-stop": {
          "$type": "color",
          "$value": "{color.context.danger.base-color}"
        }
      },
      "page": {
        "bg-body": {
          "$type": "color",
          "$value": "{color.context.default.bg-even}"
        },
        "bg-section": {
          "$type": "color",
          "$value": "{color.context.default.bg-main}"
        }
      },
      "table": {
        "default": {
          "bg-highlight": {
            "$type": "color",
            "$value": "{color.palette.info.10}"
          },
          "indicator-main": {
            "$type": "color",
            "$value": "{color.context.info.icon-main}"
          }
        },
        "alternate": {
          "bg-highlight": {
            "$type": "color",
            "$value": "{color.palette.info.90}"
          },
          "indicator-main": {
            "$type": "color",
            "$value": "{color.context.info.icon-main}"
          }
        }
      }

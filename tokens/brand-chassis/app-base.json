          "$value": "{borderRadius.context.round-40}"
        },
        "segment-item": {
          "$type": "borderRadius",
          "$value": "{borderRadius.context.round-40}"
        },
        "segment-frame": {
          "$type": "borderRadius",
          "$value": "{borderRadius.context.round-40}"
        },
        "calendar-date-day": {
          "$type": "borderRadius",
          "$value": "{borderRadius.context.round-24}"
        },
        "chip-item": {
          "$type": "borderRadius",

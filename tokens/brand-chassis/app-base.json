          "$value": "{color.context.black.indicator-main}"
        },
        "path-walk": {
          "$type": "color",
          "$value": "{color.context.neutral.indicator-main}"
        },
        "path-drive": {
          "$type": "color",
          "$value": "{color.common.map.path-ride}"

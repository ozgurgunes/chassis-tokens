{
  "color": {
    "common": {
      "form-input": {
        "default": {
          "focus": {
            "background": {
              "$type": "color",
              "$value": "{color.context.default.bg-main}"
            },
            "border": {
              "$type": "color",
              "$value": "{color.context.info.icon-main}"
            },
            "fg-active": {
              "$type": "color",
              "$value": "{color.context.default.fg-main}"
            },
            "fg-inactive": {
              "$type": "color",
              "$value": "{color.context.default.fg-subtle}"
            },
            "assist": {
              "$type": "color",
              "$value": "{color.context.info.icon-main}"
            }
          }
        },
        "alternate": {
          "focus": {
            "background": {
              "$type": "color",
              "$value": "{color.context.alternate.bg-main}"
            },
            "border": {
              "$type": "color",
              "$value": "{color.context.info.icon-main}"
            },
            "fg-active": {
              "$type": "color",
              "$value": "{color.context.alternate.fg-main}"
            },
            "fg-inactive": {
              "$type": "color",
              "$value": "{color.context.alternate.fg-subtle}"

{
  "figma": {
    "switch": {
      "brand": {
        "mode-1": {
          "$type": "boolean",
          "$value": "true"
        }
      }
    }
  },
  "color": {
    "base": {
      "palette": {
        "neutral": {
          "main": {
            "$type": "color",
            "$value": "#6F8085"
          }
        },
        "danger": {
          "main": {
            "$type": "color",
            "$value": "#F52314"
          }
        },
        "success": {
          "main": {
            "$type": "color",
            "$value": "#10C263"
          }
        },
        "warning": {
          "main": {
            "$type": "color",
            "$value": "#F5A300"
          },
          "contrast": {
            "$type": "color",
            "$value": "{color.base.palette.black.main}"
          }
        },
        "info": {
          "main": {
            "$type": "color",
            "$value": "#1414F5"
          }
        },
        "brand": {
          "main": {
            "$type": "color",
            "$value": "#00A4CC"
          }
        },
        "accent": {
          "main": {
            "$type": "color",
            "$value": "#F55200"
          }
        }
      }
    }
  },
  "typography": {
    "fontFamily": {
      "text": {
        "$type": "fontFamilies",
        "$value": "Inter, system-ui, -apple-system, Helvetica, Arial, sans-serif"
      },
      "display": {
        "$type": "fontFamilies",
        "$value": "Outfit, Georgia, 'Times New Roman', Times, serif"
      },
      "code": {
        "$type": "fontFamilies",
        "$value": "Fira Code, Menlo, Monaco, Consolas, 'Courier New', monospace"
      },
      "icon": {
        "$type": "fontFamilies",
        "$value": "Chassis-Icons"
      }
    },
    "fontWeight": {
      "text": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Medium"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Semi Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Light"
        }
      },
      "display": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "SemiBold"
        },
        "mass": {
          "$type": "fontWeights",
          "$value": "Bold"
        },
        "elegant": {
          "$type": "fontWeights",
          "$value": "Light"
        }
      },
      "code": {
        "normal": {
          "$type": "fontWeights",
          "$value": "Regular"
        },
        "strong": {
          "$type": "fontWeights",
          "$value": "Bold"
        }
      }
    }
  },
  "borderRadius": {
    "base": {
      "xsmall": {
        "$type": "borderRadius",
        "$value": "{size.base.2}"
      },
      "small": {
        "$type": "borderRadius",
        "$value": "{size.base.4}"
      },
      "medium": {
        "$type": "borderRadius",
        "$value": "{size.base.6}"
      },
      "large": {
        "$type": "borderRadius",
        "$value": "{size.base.8}"
      },
      "xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.12}"
      },
      "2xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.16}"
      },
      "3xlarge": {
        "$type": "borderRadius",
        "$value": "{size.base.20}"
      },
      "rare": {
        "$type": "borderRadius",
        "$value": "{size.base.5}"
      }
    }
  }
}
